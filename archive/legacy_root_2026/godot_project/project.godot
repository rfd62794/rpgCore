config_version=5

[application]

config/name="rpgCore NEAT Asteroids Demo"
config/description="NEAT AI learning to play Asteroids - Powered by rpgCore Game Engine"
config/version="0.1.0"
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.6", "C#", "Forward Plus")
